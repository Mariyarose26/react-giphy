{
  "extends": "airbnb",
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "quotes": "off",
    "react/prop-types": 0,
    "linebreak-style": ["error", "windows"],  // changes the file to CRLF
    "arrow-body-style": 0,
    "space-before-function-paren": 0,
    "array-bracket-spacing": "off"
  },
  "env": {
    "browser": true
  },
  "parser": "babel-eslint"
}
